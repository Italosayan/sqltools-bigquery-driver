{
  "name": "@evidence-dev/sqltools-bigquery-driver",
  "displayName": "BigQuery Driver for SQLTools",
  "description": "SQLTools BigQuery Driver",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "Evidence",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/evidence-dev/sqltools-bigquery-driver"
  },
  "icon": "default.png",
  "bugs": {
    "url": "https://github.com/evidence-dev/sqltools-bigquery-driver/issues"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "keywords": [
    "driver-template",
    "sqltools-driver"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "@sqltools/base-driver": "latest",
    "@sqltools/types": "latest",
    "uuid": "^7.0.2",
    "@google-cloud/bigquery": "6.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/node": "^14.0.9",
    "@types/vscode": "^1.42.0",
    "typescript": "^3.7.3"
  }
}
